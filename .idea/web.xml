<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                             http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <servlet>
        <servlet-name>StudentFormServlet</servlet-name>
        <servlet-class>com.studentprofile.StudentFormServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>StudentFormServlet</servlet-name>
        <url-pattern>/submit</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DisplayStudentServlet</servlet-name>
        <servlet-class>com.studentprofile.DisplayStudentServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>DisplayStudentServlet</servlet-name>
        <url-pattern>/displayStudent</url-pattern>
    </servlet-mapping>

</web-app>
